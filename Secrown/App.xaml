<Application x:Class="Secrown.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Secrown"
             xmlns:vm="clr-namespace:Secrown.ViewModels"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <vm:MainWindow x:Key="MainWindow"/>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <SolidColorBrush x:Key="LightBlue">#03A5F0</SolidColorBrush>
        <SolidColorBrush x:Key="DarkBlue">#2E86C1</SolidColorBrush>
        <SolidColorBrush x:Key="Blue">#3498db</SolidColorBrush>
        <SolidColorBrush x:Key="LightGray">#F1F1F1</SolidColorBrush>
        <SolidColorBrush x:Key="SemiBlack">#2D2D30</SolidColorBrush>
    </Application.Resources>
</Application>
